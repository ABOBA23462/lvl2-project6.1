<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="73dp"
        android:layout_marginStart="29dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/person"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="70.33dp"
        android:layout_marginEnd="30.33dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rent a car "
        android:textSize="42sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="122dp"
        android:layout_marginStart="25dp"/>

    <ImageView
        android:id="@+id/tab2"
        android:background="@drawable/rounder_tab"
        android:layout_width="310dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="198dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lupa"
        app:layout_constraintTop_toTopOf="@id/tab2"
        app:layout_constraintStart_toStartOf="@id/tab2"
        android:layout_marginTop="15dp"
        android:layout_marginStart="16dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:textSize="22sp"
        android:textStyle="bold"
        android:textColor="@color/grey"
        app:layout_constraintTop_toTopOf="@id/tab2"
        app:layout_constraintStart_toStartOf="@id/tab2"
        android:layout_marginTop="9dp"
        android:layout_marginStart="49dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/bicycletab"
        android:layout_width="309dp"
        android:layout_height="120dp"
        app:cardCornerRadius="10dp"
        app:cardBackgroundColor="@color/super_blue"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="281dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="26dp">

        <ImageView
            android:id="@+id/arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/arrowrev"
            android:layout_marginStart="19dp"
            android:layout_marginTop="53.08dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bogamy"
            android:textSize="22dp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:layout_marginTop="78dp"
            android:layout_marginStart="15dp"/>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bicyclesml"
        app:layout_constraintStart_toStartOf="@id/bicycletab"
        app:layout_constraintTop_toTopOf="@id/bicycletab"
        android:layout_marginStart="182dp"
        android:layout_marginTop="9dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cartab"
        style="@style/Pink.CardView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="448dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="26dp"
        >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/arrowrev"
            android:layout_marginStart="19dp"
            android:layout_marginTop="53.08dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bogamy"
            android:textSize="22dp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:layout_marginTop="78dp"
            android:layout_marginStart="15dp"/>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/car"
        app:layout_constraintStart_toStartOf="@id/cartab"
        app:layout_constraintTop_toTopOf="@id/cartab"
        android:layout_marginStart="186dp"
        android:layout_marginTop="31dp"/>

    <androidx.cardview.widget.CardView
        style="@style/Blue.CardView"
        android:id="@+id/cartab2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="614dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="26dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/arrowrev"
            android:layout_marginStart="19dp"
            android:layout_marginTop="53.08dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bogamy"
            android:textSize="22dp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:layout_marginTop="78dp"
            android:layout_marginStart="15dp"/>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/cartwo"
        app:layout_constraintStart_toStartOf="@id/cartab2"
        app:layout_constraintTop_toTopOf="@id/cartab2"
        android:layout_marginStart="178dp"
        android:layout_marginTop="32dp"/>




</androidx.constraintlayout.widget.ConstraintLayout>